@page "/Spotify"
@inject HttpClient Http
@using Models

    
<h3>FetchSpotify</h3>

@code {
    WeatherInfo[] weatherInfo;

    protected async override Task OnInitializedAsync()
    {
        try
        {
            weatherInfo = await Http.GetFromJsonAsync<WeatherInfo[]>("http://www.7timer.info/bin/api.pl?lon=113.17&lat=23.09&product=astro&output=json");
            //Console.WriteLine("WeatherInfo: ", weatherInfo);
        }
        catch (Exception ex) { Console.WriteLine(ex.Message);  }


    }
}
